	public static HashMap getProvinces(){
		return provinces;
	}
	
	public static String getProvName(String provCode){
		return (String)provinces.get(provCode);
	}
	public static String getCityName(String cityCode){
		return (String)citys.get(cityCode);
	}
	
	public static HashMap getCitys(String provID){
		HashMap subCitys = new HashMap();
		String pre = provID.substring(0,2);
		Iterator it = citys.keySet().iterator();
		while(it.hasNext()){
			String citycode=(String)it.next();
			if(citycode.substring(0,2).equals(pre)){
				subCitys.put(citycode,citys.get(citycode));
			}
		}
		return subCitys;
	}

	public static List getProvincesList() {
